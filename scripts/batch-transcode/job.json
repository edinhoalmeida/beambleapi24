                  "CslgAtom": "EXCLUDE",
                  "FreeSpaceBox": "EXCLUDE",
                  "MoovPlacement": "NORMAL"
                }
              },
              "VideoDescription": {
                "Width": 720,
                "Height": 1280,
                "ScalingBehavior": "DEFAULT",
                "VideoPreprocessors": {
                  "TimecodeBurnin": {
                    "FontSize": 32,
                    "Position": "TOP_CENTER"
