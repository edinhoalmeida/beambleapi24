{
    "Role": "arn:aws:iam::979687591672:role/beamble-media-convert-role",
    "AccelerationSettings" : {
      "Mode" : "ENABLED"
    },
    "UserMetadata": {
        "job" : "Acceleration"
    },
    "Settings": {
      "TimecodeConfig": {
        "Source": "ZEROBASED"
      },
      "OutputGroups": [
        {
          "Name": "File Group",
          "Outputs": [
            {
              "ContainerSettings": {
                "Container": "MP4",
